		<PackageReference Include="AspNetCore.HealthChecks.Network" Version="8.0.1" />
		<PackageReference Include="Microsoft.EntityFrameworkCore.Analyzers" Version="8.0.14" />
	</ItemGroup>

	<ItemGroup>
		<Content Include="Files\**">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
			<CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
		</Content>
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\ScoreSharp.Common\ScoreSharp.Common.csproj" />
		<ProjectReference Include="..\ScoreSharp.RazorTemplate\ScoreSharp.RazorTemplate.csproj" />
		<ProjectReference Include="..\ScoreSharp.Watermark\ScoreSharp.Watermark.csproj" />
	</ItemGroup>

	<ItemGroup>
		<None Include="efpt.config.json.user" />
	</ItemGroup>


</Project>
