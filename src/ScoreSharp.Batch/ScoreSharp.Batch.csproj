    	<PackageReference Include="FluentFTP" Version="52.1.0" />
    	<PackageReference Include="FluentFTP.Logging" Version="1.0.0" />
		<PackageReference Include="Hangfire.AspNetCore" Version="1.8.17" />
		<PackageReference Include="Hangfire.SqlServer" Version="1.8.17" />
		<PackageReference Include="Microsoft.Data.SqlClient" Version="5.2.1" />
		<PackageReference Include="Microsoft.Extensions.Http.Resilience" Version="8.10.0" />
		<PackageReference Include="Microsoft.Extensions.Resilience" Version="8.10.0" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
		<PackageReference Include="QRCoder" Version="1.6.0" />
		<PackageReference Include="Serilog.AspNetCore" Version="8.0.2" />
		<PackageReference Include="Hangfire.Console.Extensions.Serilog" Version="2.0.0" />
		<PackageReference Include="NSwag.AspNetCore" Version="14.0.7" />
		<PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="8.0.14" />
		<PackageReference Include="TemplateEngine.Docx" Version="1.1.5" />
		<PackageReference Include="Ulid" Version="1.3.4" />
		<PackageReference Include="FluentEmail.Core" Version="3.0.2" />
